{
    "compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es2017",
		"lib": ["es2015", "dom"],
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": true,
		"allowSyntheticDefaultImports": true
	}
}
